function calculadora(){
    
    let dados = {

        soma(number1, number2){
            return number1 + number2;
        },

        subtracao(number1, number2){
            return number1 - number2;
        },

        multiplicacao(number1, number2){
            return number1 * number2;
        },

        divisao(number1, number2){
            return number1 / number2;
        },


    }
    return dados;
}
